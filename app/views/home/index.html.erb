<div class="container">
  <%= render "include/header" %>

  <ul class="nav nav-pills nav-fill" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" id="hn-tab" href="#hn" data-toggle="pill" role="tab" aria-controls="hn" aria-selected="true">Hacker News</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="reddit-tab" href="#reddit" data-toggle="pill" role="tab" aria-controls="reddit" aria-selected="false">r/programming</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="sd-tab" href="#sd" data-toggle="pill" role="tab" aria-controls="sd" aria-selected="false">Slashdot</a>
    </li>
  </ul>
  <br>
  <div class="tab-content" id="content">
    <div class="tab-pane fade show active" id="hn" role="tabpanel" aria-labelledby="hn-tab">
      <%= render "include/hn_content" %>
    </div>
    <div class="tab-pane fade" id="reddit" role="tabpanel" aria-labelledby="reddit-tab">
      <%= render "include/reddit_content" %>
    </div>
    <div class="tab-pane fade" id="sd" role="tabpanel" aria-labelledby="sd-tab">
      <%= render "include/sd_content" %>
    </div>
  </div>
</div>



